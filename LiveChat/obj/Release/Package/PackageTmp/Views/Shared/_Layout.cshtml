<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title - Hong Dai Website</title>
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/themes/base/css")
    <link href="~/Content/Home.css" rel="stylesheet" />
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div id="Home-Head">
        <span>Test User Interface</span>
    </div>
    @RenderBody()

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryui")
    @RenderSection("scripts", required: false)

    <footer>
        <div>
            <div>
                <p>&copy; @DateTime.Now.Year - Hong Dai Website</p>
            </div>
            <div>
                <ul>
                    <li><a target="_blank" href="http://ca.linkedin.com/in/HongDai">LinkedIn</a></li>
                </ul>
            </div>
        </div>
    </footer>
</body>
</html>
